class Prime 
{
	public static void main(String[] args) 
	{
		int n=7;
		boolean isprime=true;
		for(int i=2;i<=n;i++)
		{
			if(n%2==0)
			{
				isprime=false;
				break;
			}
		}
		if(isprime==true)
		{
			System.out.println(n+"is prime number");
		}
		else
		System.out.println(n+"is not a prime number");
	}
}
